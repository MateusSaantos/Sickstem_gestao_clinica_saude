{
    "name": "sickstem/clinica_sickstem",
    "description": "Gerenciamento de uma clinica de saude",
    "type": "project",
    "require": {
      "php": ">=7.4",
      "illuminate/database": "^10.0"
    },
    "autoload": {
      "psr-4": {
        "App\\": "src/"
      }
    },
    "scripts": {
      "start": "php -S localhost:8000 -t public"
    },
    "config": {
      "optimize-autoloader": true,
      "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    }
}
